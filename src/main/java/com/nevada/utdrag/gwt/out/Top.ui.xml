<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style src="../scaffold/MyUi.css" />
	
	<ui:image field="front_logo"></ui:image>
	
	<ui:style field='images'>
		@sprite .front_logo {
			gwt-image: 'front_logo';
			float: left;
		}
	</ui:style>
	
	<g:HTMLPanel>
		<div class='{style.top}'>

			<div class='{style.buttonDiv}'>			
				<g:Anchor styleName='{images.front_logo}' href='javascript:;'
					ui:field="logo"></g:Anchor>
			<div class='{style.title}'>Sveriges största och enklaste "utdragstjänst"</div>
			</div>

			<div class='{style.buttonDiv}'>
				<g:Anchor styleName='{style.button}' href='javascript:;'
					ui:field="about">om oss</g:Anchor>
				<g:Anchor styleName='{style.button}' href='javascript:;'
					ui:field="policy">sekresspolicy</g:Anchor>
				<g:Anchor styleName='{style.button}' href='javascript:;'
					ui:field='blogg'>blogg</g:Anchor>
				<g:Anchor styleName='{style.button}' href='javascript:;'
					ui:field='how'>så funkar det</g:Anchor>
			</div>

		</div>
	</g:HTMLPanel>
</ui:UiBinder> 